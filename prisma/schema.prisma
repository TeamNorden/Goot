generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["debian-openssl-1.1.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model guild {
  id         String @unique
  name       String
}

model commands {
  placeholder String @unique @default("//k/")
  guild String
  text String?
  title String?
  description String?
  deltrig Boolean?
  permission String?
  cmd String?
}

model custombot {
    id String @unique
    botname String
    botavatar String
}
